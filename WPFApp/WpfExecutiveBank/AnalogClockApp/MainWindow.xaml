<Window x:Class="AnalogClockApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnalogClockApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="350" Initialized="Window_Initialized">
    <Grid Margin="10">
        <Canvas x:Name="CvsClock" Width="250" Height="250">
            <Ellipse x:Name="ElsClock" Width="250" Height="250" />
        </Canvas>

    </Grid>
</Window>
